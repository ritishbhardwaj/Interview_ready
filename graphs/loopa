def sol():
    pass

n=4
grid=[["" for i in range((n*2)-1)] for i in range(n)]
num=1
for row in range(n):
    for col in range((row*2)+1):
        if row%2==0:
            grid[row][col]=num
            grid[row][col+1]="*"
        else:
            grid[row][col]=num
            grid[row][col]
        num+=1
for i in grid:
    print(grid)